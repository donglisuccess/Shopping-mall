
.contain{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 100rpx;
	box-shadow: 0px 0px 5px #C0C0C0;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding-top: 50rpx;
	background-color: white;
	position: fixed;
	left: 0px;
	right: 0px;
	top: 0px;
	z-index: 1;
}
.left{
	width: 120rpx;
	text-align: center;
}
.center{
	width : 510rpx;
}
.right{
	width: 120rpx;
}

